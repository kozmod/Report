            <GridPane.margin>
                <Insets right="90.0"/>
            </GridPane.margin>
        </Text>
        <Label fx:id="typeHomeLabel" text="Type" textFill="#902727" GridPane.columnSpan="2" GridPane.halignment="RIGHT"
