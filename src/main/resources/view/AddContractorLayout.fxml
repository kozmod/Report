            <GridPane.margin>
                <Insets right="15.0"/>
            </GridPane.margin>
        </Label>
        <Label id="errorLabel" fx:id="errorLabel" text="Label" textFill="RED" visible="false" GridPane.columnSpan="2"
               GridPane.halignment="CENTER" GridPane.rowIndex="6">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
    </children>


</GridPane>
